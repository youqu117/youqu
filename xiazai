    <script>
        function switchTab(tabName) {
            // éšè—æ‰€æœ‰æ ‡ç­¾å†…å®¹
            document.querySelectorAll('.tab-content').forEach(tab => {
                tab.classList.remove('active');
            });
            
            // ç§»é™¤æ‰€æœ‰æ ‡ç­¾çš„æ´»åŠ¨çŠ¶æ€
            document.querySelectorAll('.tab').forEach(tab => {
                tab.classList.remove('active');
            });
            
            // æ˜¾ç¤ºé€‰ä¸­çš„æ ‡ç­¾å†…å®¹
            document.getElementById(tabName).classList.add('active');
            
            // è®¾ç½®é€‰ä¸­çš„æ ‡ç­¾ä¸ºæ´»åŠ¨çŠ¶æ€
            event.currentTarget.classList.add('active');
        }
        
        // ä¸ºä¸‹è½½æŒ‰é’®æ·»åŠ ç‚¹å‡»äº‹ä»¶ï¼ˆæ¨¡æ‹Ÿä¸‹è½½ï¼‰
        document.querySelector('.btn').addEventListener('click', function(e) {
            if (!document.querySelector('.btn').getAttribute('href')) {
                e.preventDefault();
                alert('åœ¨å®é™…çš„ GitHub é¡µé¢ä¸­ï¼Œæ­¤æŒ‰é’®ä¼šä¸‹è½½ image rename.pyw æ–‡ä»¶ã€‚');
            }
        });
    </script>
